{
  "name": "directus-gatsby",
  "version": "0.1.0",
  "description": "Gatsby static site generator configured to use Directus headless CMS",
  "main": "index.js",
  "keywords": [
    "directus",
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tbalsys/directus-gatsby"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "author": "Tomas Balsys",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --host 0.0.0.0 --port 8003",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve --host 0.0.0.0 --port 8004",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@directus/gatsby-source-directus": "^0.2.6",
    "commento-react": "^1.2.4-beta",
    "gatsby": "^2.23.12",
    "gatsby-image": "^2.4.9",
    "gatsby-plugin-manifest": "^2.4.14",
    "gatsby-plugin-offline": "^3.2.13",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-source-filesystem": "^2.3.14",
    "gatsby-transformer-sharp": "^2.5.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.0.0",
    "dotenv": "^8.2.0",
    "gatsby-cli": "^2.12.59",
    "prettier": "2.0.5"
  }
}
